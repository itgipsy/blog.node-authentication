{
  "extends": "eslint:recommended",
  "globals": {
    "document": true
  },
  "rules": {
    "eqeqeq": 1,
    "no-console": 0,
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
   },
   "parserOptions": {
      "ecmaVersion": 6
   },
  "env": {
    "es6": true,
    "node": true,
    "jquery": true
  }
}
